do
    let r1: Result[Number,String] := { Success 1.[Number] },
    let r2: Result[Number,String] := { Failure 'bad' },
    let s1: String :=
        switch r1.(Success):
        case Some num:
            num.{str},
        default:
            'null',
        end,
    let s2: String :=
        switch r2.(Success):
        case Some num:
            num.{str},
        default:
            'null',
        end,
    let s3: String :=
        switch r2.(NG):
        case Some err:
            err.{str},
        default:
            'null',
        end,
    { println { "#,#,#" (s1,s2,s3) } }
        . { crash-on-error };

