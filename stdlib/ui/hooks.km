type Hook[T] (&(&(T)(Component))(Component));

public function use[T]: ( &(Hook[T],(&(T)(Component))) Component )
    ( &(hook,k)
        k | hook
    );

public function ui-list[T]: ( &(Reactive[List[T]]) Hook[Reactive[T]] )
    ( &(list)
    ( &(k)
        dom-dynamic 'div'
            ~ consume &(item) := list,
            k item
    ));
