type Bit     native;  // bool
type Byte    Uint8;
type Word    Uint16;
type Dword   Uint32;
type Qword   Uint64;
type Bytes   protected Array[Byte];  // []byte

type BinaryError native;
public function from: ( &(BinaryError) String )
    native 'String from error';
public function crash: ( &(BinaryError) Action )
    ( &(err) (crash ((str err) | "Binary Error: #")) );
