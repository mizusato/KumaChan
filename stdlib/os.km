module OS;

public const Stdin := [I/O::InputStream] native 'Stdin';

public const Stdout := [I/O::OutputStream] native 'Stdout';

public const Stderr := [I/O::OutputStream] native 'Stderr';

type ProcessExit  protected Effect[(),()];

public function exit {
    lambda(Uint8) ProcessExit:
    native 'exit'
};
